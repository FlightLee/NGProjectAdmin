    "Interval": 10,
    //超时时间
    "Timeout": 1
  }, //SignalR配置
  "SignalRConfig": {
    //是否启用
    "IsEnabled": false,
    //pattern
    "Pattern": "/API/ChatHub",
    //method
    "Method": "Show"
  }, //Quartz定时任务配置
  "QuartzConfig": {
    //RuYiAdminFrameworkJob调度间隔
    "NGAdminFrameworkJob": "0/5 * * * * ?",
    //Quartz作业工作组名称
    "ScheduleJobGroup": "NGAdmin.JobGroup",
    //Quartz作业触发器名称
    "ScheduleJobTrigger": "NGAdmin.JobTrigger",
    //是否支持集群
    "SupportGroup": true,
    //集群编号
    "GroupId": "1",
    //订阅redis通道名称
    "ChanelName": "QuartzGroupChanel"
